SERVICE_LEVEL=""
# Description: Define service level
# Valid input: CTS-AI, CTS-E, CTS-S or PREPARE
# Example: CTS-AI

FORMAT_DATA_DISK=""
# Description: Should the installer format a data disk?
# Valid input: YES or NO
# Example: YES

DATA_DISK_PATH=""
# Description: Full path to data disk
# Example: /dev/nvme1n1

DELETE_DATA_DISK=""
# Description: Should we delete everything on the data disk? Answer NO will exit the installer
# Valid input: YES or NO
# Example: YES

NETWORK_RECORDER_LOCATION=""
# Description: Network recorder storage location. DISK (normal use case) will use existing data disk and RAM will use a RAM disk
# Valid input: DISK or RAM
# Example: DISK

CREATE_SUPPORT_USER=""
# Description: Option to create NTT support user (nttsecurity)
# Valid input: YES or NO
# Example: Yes

SUPPORT_USER_PASSWORD="" 
# Description: Password for nttsecurity support user
# Example: S0m3 thING# r@ndOOM

NTP1="" 
# Description: Primary NTP server
# Example: 0.se.pool.ntp.org

NTP2=""
# Description: Secondary NTP server, type NONE to skip
# Valid input: hostname or NONE
# Example: 1.se.pool.ntp.org

MONITORING_INTERFACE=""
# Description: Define one or more monitoring interfaces, separated with "," (no space)
# Example: dummy0,ens1

ALLOW_HTTP=""
# Description: Allow temporary access to HTTP during enrolment
# Valid input: YES or NO
# Example: YES

INITKEY=""
# Description: Type init key

DEVICENAME=""
# Description: Define wanted device name, will be used to identify the device in reports
